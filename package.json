{
  "name": "core-web-v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "generate-contract": "graphql-codegen",
    "dev": "next dev",
    "build": "next build",
    "start": "NODE_OPTIONS='-r next-logger' next start",
    "start:simulate": "NODE_OPTIONS='-r next-logger' next start | pino-pretty",
    "lint": "eslint",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook --disable-emoji || true",
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format",
      "git add"
    ]
  },
  "dependencies": {
    "@99/helper": "0.3.0",
    "@apollo/client": "^3.3.21",
    "@apollo/react-ssr": "4.0.0",
    "cors": "^2.8.5",
    "graphql": "^15.5.1",
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rosetta": "1.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@graphql-codegen/cli": "^2.0.1",
    "@graphql-codegen/typescript": "^2.0.0",
    "@graphql-codegen/typescript-operations": "^2.0.1",
    "@types/node": "^16.4.3",
    "@types/react": "^17.0.15",
    "@typescript-eslint/eslint-plugin": "4.29.0",
    "commitizen": "^4.2.4",
    "eslint": "7.31.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-require-jsdoc-except": "1.5.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-sort-destructure-keys": "1.3.5",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "1.7.0",
    "git-cz": "4.7.6",
    "husky": "4.3.8",
    "lint-staged": "11.1.2",
    "next-logger": "^2.1.0",
    "prettier": "2.3.2",
    "typescript": "^4.3.5"
  }
}
